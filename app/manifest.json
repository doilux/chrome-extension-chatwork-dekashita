{
  "name": "Chatwork Dekashita",
  "version": "0.0.1",
  "manifest_version": 2,
  "description": "Chatworkで「でかした！！」と入力するボタン",
  "icons": {
    "16": "images/maruta-16.png",
    "128": "images/maruta-128.png"
  },
  "default_locale": "en",
  "content_scripts": [{
    "matches": [ "https://www.chatwork.com/*", "https://kcw.kddi.ne.jp/*" ],
    "js": [ "bower_components/jquery/dist/jquery.min.js", "dist/js/dekashita.js" ]
  }],
  "background": {
    "scripts": [
      "dist/js/chromereload.js",
      "dist/js/background.js"
    ],
    "persistent": false
  },
  "permissions": [],
  "web_accessible_resources": [
    "dist/img/*.png"
  ]
}
